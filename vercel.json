{
    "version": 2,
    "builds": [
     {
      "src": "server.js",
      "use": "@vercel/node"
     }
    ],
    "routes": [
     {
      "src": "/(.*)",
      "dest": "server.js"
     },
     {
        "src": "/api/products(.*)",
        "dest": "/src/features/product/routes/product.routes.js"
     },
     {
        "src": "/api/users(.*)",
        "dest": "/src/features/user/routes/user.routes.js"
     },
     {
        "src": "/api/cartItems(.*)",
        "dest": "/src/features/cart/cartitems.routes.js"
     }
    ]
   }